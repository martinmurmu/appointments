<h1>Listing managers</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Phone</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @managers.each do |manager| %>
  <tr>
    <td><%= manager.name %></td>
    <td><%= manager.phone %></td>
    <td><%= link_to 'Show', admin_manager_path(manager) %></td>
    <td><%= link_to 'Edit', edit_admin_manager_path(manager) %></td>
    <td><%= link_to 'Destroy', admin_manager_path(manager), method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Manager', new_admin_manager_path %>
