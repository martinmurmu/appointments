<p id="notice"><%= notice %></p>

<div class="row">
  <div class="col-lg-12">
    <section class="panel">
      <header class="panel-heading">
        Manager Information
      </header>
      <form class="form-horizontal">
        <div class="form-group">
          <label class="col-lg-2 control-label">Practice Name</label>
          <div class="col-lg-10">
            <p class="form-control-static"><%= @manager.practice_name %></p>
          </div>
        </div>
        <div class="form-group">
          <label class="col-lg-2 control-label">Practice Address</label>
          <div class="col-lg-10">
            <p class="form-control-static"><%= @manager.practice_address %></p>
          </div>
        </div>
        <div class="form-group">
          <label class="col-lg-2 control-label">Practice Phone</label>
          <div class="col-lg-10">
            <p class="form-control-static"><%= @manager.practice_phone %></p>
          </div>
        </div>
        <div class="form-group">
          <div class="col-lg-offset-2 col-lg-10">
            <%= link_to 'Edit', edit_manager_manager_path(@manager), :class => "btn btn-default" %>
          </div>
        </div>
      </form>
    </section>
  </div>
</div>

<%= link_to 'Back', manager_managers_path %>

